{
  "name": "Ozonator",
  "description": "Cleanup an arrange some Ozone market pages elements",
  "action": {
    "default_icon": "ozon-shine.png"
  },
  "manifest_version": 3,
  "version": "0.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "js/service-worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ozon.ru/my/orderlist*"
      ],
      "js": [
      ],
      "css": [
        "css/styles.css"
      ]
    }
  ]
}